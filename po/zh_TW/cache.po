# File: docs/8.x/cache.md, line: 231
# File: docs/master/cache.md, line: 231
#. type: Plain text
#, no-wrap
msgid "    Cache::put('key', 'value', 0);\n"
msgstr ""

# File: docs/8.x/cache.md, line: 233
# File: docs/master/cache.md, line: 233
#. type: Plain text
#, no-wrap
