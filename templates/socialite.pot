# File: docs/8.x/socialite.md, line: 69
# File: docs/master/socialite.md, line: 69
#, no-wrap
msgid "Authentication & Storage"
msgstr ""
#
#. type: Plain text
# File: docs/8.x/socialite.md, line: 72
# File: docs/master/socialite.md, line: 72
msgid "Once the user has been retrieved from the OAuth provider, you may determine if the user exists in your application's database and [authenticate the user](/docs/{{version}}/authentication#authenticate-a-user-instance). If the user does not exist in your application's database, you will typically create a new record in your database to represent the user:"
msgstr ""
#
#. type: Plain text
# File: docs/8.x/socialite.md, line: 76
# File: docs/master/socialite.md, line: 76
#, no-wrap
msgid ""
"    use App\\Models\\User;\n"
"    use Illuminate\\Support\\Facades\\Auth;\n"
"    use Laravel\\Socialite\\Facades\\Socialite;\n"
msgstr ""
#
#. type: Plain text
# File: docs/8.x/socialite.md, line: 79
# File: docs/master/socialite.md, line: 79
#, no-wrap
msgid ""
"    Route::get('/auth/callback', function () {\n"
"        $githubUser = Socialite::driver('github')->user();\n"
msgstr ""
#
#. type: Plain text
# File: docs/8.x/socialite.md, line: 81
# File: docs/master/socialite.md, line: 81
#, no-wrap
msgid "        $user = User::where('github_id', $githubUser->id)->first();\n"
msgstr ""
#
#. type: Plain text
# File: docs/8.x/socialite.md, line: 96
# File: docs/master/socialite.md, line: 96
#, no-wrap
msgid ""
"        if ($user) {\n"
"            $user->update([\n"
"                'github_token' => $githubUser->token,\n"
"                'github_refresh_token' => $githubUser->refreshToken,\n"
"            ]);\n"
"        } else {\n"
"            $user = User::create([\n"
"                'name' => $githubUser->name,\n"
"                'email' => $githubUser->email,\n"
"                'github_id' => $githubUser->id,\n"
"                'github_token' => $githubUser->token,\n"
"                'github_refresh_token' => $githubUser->refreshToken,\n"
"            ]);\n"
"        }\n"
msgstr ""
#
#. type: Plain text
# File: docs/8.x/socialite.md, line: 98
# File: docs/master/socialite.md, line: 98
#, no-wrap
msgid "        Auth::login($user);\n"
msgstr ""
#
#. type: Plain text
# File: docs/8.x/socialite.md, line: 101
# File: docs/master/socialite.md, line: 101
#, no-wrap
msgid ""
"        return redirect('/dashboard');\n"
"    });\n"
msgstr ""
#
#. type: Plain text
# File: docs/8.x/socialite.md, line: 103
# File: docs/master/socialite.md, line: 103
#, no-wrap
msgid "> {tip} For more information regarding what user information is available from specific OAuth providers, please consult the documentation on [retrieving user details](#retrieving-user-details).\n"
msgstr ""
#
#. type: Plain text
# File: docs/8.x/socialite.md, line: 105
