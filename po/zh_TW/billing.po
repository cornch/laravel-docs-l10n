msgctxt "CODE"
msgid ""
"use Illuminate\\Http\\Request;\n"
"\n"
"Route::post('/user/subscribe', function (Request $request) {\n"
"    $request->user()->newSubscription('default')\n"
"        ->meteredPrice('price_metered')\n"
"        ->create($request->paymentMethodId);\n"
"\n"
"    // ...\n"
"});"
msgstr ""

