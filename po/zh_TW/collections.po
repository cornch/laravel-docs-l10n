"    'address.state' => 'MI',\n"
"    'address.postcode' => '48219'\n"
"]);\n"
"\n"
"$person = $person->undot();\n"
"\n"
"$person->toArray();\n"
"\n"
"/*\n"
"    [\n"
"        \"name\" => [\n"
"            \"first_name\" => \"Marie\",\n"
"            \"last_name\" => \"Valentine\",\n"
"        ],\n"
"        \"address\" => [\n"
"            \"line_1\" => \"2992 Eagle Drive\",\n"
"            \"line_2\" => \"\",\n"
"            \"suburb\" => \"Detroit\",\n"
"            \"state\" => \"MI\",\n"
