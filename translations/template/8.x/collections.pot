#: docs/8.x/collections.md:3074 docs/8.x/collections.md:3096
#, no-wrap
msgid ""
"    $collection = collect([\n"
"        ['name' => 'Desk'],\n"
"        ['name' => null],\n"
"        ['name' => 'Bookcase'],\n"
"    ]);\n"
msgstr ""

#. type: Plain text
#: docs/8.x/collections.md:3076
#, no-wrap
msgid "    $filtered = $collection->whereNotNull('name');\n"
msgstr ""

#. type: Plain text
#: docs/8.x/collections.md:3085
#, no-wrap
msgid ""
"    /*\n"
"        [\n"
"            ['name' => 'Desk'],\n"
"            ['name' => 'Bookcase'],\n"
"        ]\n"
"    */\n"
msgstr ""

#. type: Plain text
#: docs/8.x/collections.md:3087
#, no-wrap
msgid "<a name=\"method-wherenull\"></a>\n"
