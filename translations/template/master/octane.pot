msgstr ""

#. type: Plain text
#: docs/master/octane.md:262
#, no-wrap
msgid "php artisan octane:start --max-requests=250\n"
msgstr ""

#. type: Plain text
#: docs/master/octane.md:267
#, no-wrap
msgid "<a name=\"reloading-the-workers\"></a>\n"
