"use Illuminate\\Database\\Eloquent\\Relations\\MorphTo;\n"
"\n"
"$activities = ActivityFeed::with([\n"
"    'parentable' => function (MorphTo $morphTo) {\n"
"        $morphTo->morphWithCount([\n"
"            Photo::class => ['tags'],\n"
"            Post::class => ['comments'],\n"
"        ]);\n"
"    }])->get();"
