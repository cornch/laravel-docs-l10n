"        if ($this->app->environment('local')) {\n"
"            return true;\n"
"        }\n"
"\n"
"        return $entries->contains(function (IncomingEntry $entry) {\n"
"            return $entry->isReportableException() ||\n"
"                $entry->isFailedJob() ||\n"
"                $entry->isScheduledTask() ||\n"
"                $entry->isSlowQuery() ||\n"
"                $entry->hasMonitoredTag();\n"
"            });\n"
