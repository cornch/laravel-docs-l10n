msgid "    $collection = collect(['first', 'second']);\n"
msgstr ""
#
#. type: Plain text
# File: docs/8.x/collections.md, line: 53
# File: docs/master/collections.md, line: 53
#, no-wrap
msgid "    $upper = $collection->toUpper();\n"
msgstr ""
#
#. type: Plain text
