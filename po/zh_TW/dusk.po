"use Illuminate\\Foundation\\Testing\\DatabaseMigrations;\n"
"use Laravel\\Dusk\\Chrome;\n"
"use Tests\\DuskTestCase;\n\n"
"class ExampleTest extends DuskTestCase\n"
"{\n"
"    use DatabaseMigrations;\n"
"}"
msgstr ""

