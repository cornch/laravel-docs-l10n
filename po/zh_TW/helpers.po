"\n"
"$array = [\n"
"    'user.name' => 'Kevin Malone',\n"
"    'user.occupation' => 'Accountant',\n"
"];\n"
"\n"
"$array = Arr::undot($array);\n"
"\n"
"// ['user' => ['name' => 'Kevin Malone', 'occupation' => 'Accountant']]"
