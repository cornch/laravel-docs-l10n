"use Laravel\\Fortify\\Fortify;\n"
"\n"
"/**\n"
" * Bootstrap any application services.\n"
" *\n"
" * @return void\n"
" */\n"
"public function boot()\n"
"{\n"
"    Fortify::confirmPasswordView(function () {\n"
"        return view('auth.confirm-password');\n"
"    });\n"
"\n"
"    // ...\n"
"}"
