# File: docs/9.x/migrations.md, line: 1183
# File: docs/master/migrations.md, line: 1183
#, no-wrap
msgid "    $table->dropForeign(['user_id']);\n"
msgstr ""
#
#. type: Plain text
# File: docs/8.x/migrations.md, line: 1163
# File: docs/9.x/migrations.md, line: 1185
# File: docs/master/migrations.md, line: 1185
