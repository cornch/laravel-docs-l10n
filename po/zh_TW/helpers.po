"$joined = Arr::join($array, ', ');\n"
"\n"
"// Tailwind, Alpine, Laravel, Livewire\n"
"\n"
"$joined = Arr::join($array, ', ', ' and ');\n"
"\n"
"// Tailwind, Alpine, Laravel and Livewire"
