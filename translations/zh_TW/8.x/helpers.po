#. type: Plain text
#: docs/8.x/helpers.md:2182
#, no-wrap
msgid ""
"    $replaced = Str::of('123')->replaceMatches('/\\d/', function ($match) {\n"
"        return '['.$match[0].']';\n"
"    });\n"
msgstr ""

