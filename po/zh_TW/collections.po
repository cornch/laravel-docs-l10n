"$collection = collect([\n"
"    [\n"
"        'name' => 'John Doe',\n"
"        'department' => 'Sales',\n"
"    ],\n"
"    [\n"
"        'name' => 'Jane Doe',\n"
"        'department' => 'Sales',\n"
