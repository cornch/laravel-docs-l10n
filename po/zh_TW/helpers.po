"$hasError = true;\n"
"\n"
"$array = ['p-4', 'font-bold' => $isActive, 'bg-red' => $hasError];\n"
"\n"
"$classes = Arr::toCssClasses($array);\n"
"\n"
"/*\n"
"    'p-4 bg-red'\n"
"*/"
msgstr ""

