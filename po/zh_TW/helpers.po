"use Illuminate\\Support\\Arr;\n"
"\n"
"$array = [100, '200', 300, '400', 500];\n"
"\n"
"$filtered = Arr::where($array, function ($value, $key) {\n"
"    return is_string($value);\n"
"});\n"
"\n"
"// [1 => '200', 3 => '400']"
