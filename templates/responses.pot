"Route::middleware('cache.headers:public;max_age=2628000;etag')->group(function () {\n"
"    Route::get('/privacy', function () {\n"
"        // ...\n"
"    });\n"
"\n"
"    Route::get('/terms', function () {\n"
"        // ...\n"
"    });\n"
"});"
