#: ./docs/10.x/pagination.md:188
#: ./docs/11.x/pagination.md:188
#: ./docs/master/pagination.md:188
msgid "use App\\Models\\User;\n\n"
"Route::get('/users', function () {\n"
"    $users = User::paginate(15);\n\n"
"    $users->appends(['sort' => 'votes']);\n\n"
"    // ...\n"
"});"
msgstr ""

# P
