" */\n"
"protected function rules()\n"
"{\n"
"    return [\n"
"        'avatar' => [\n"
"            ...$this->isPrecognitive() ? [] : ['required'],\n"
"            'image',\n"
"            'mimes:jpg,png',\n"
"            'dimensions:ratio=3/2',\n"
"        ],\n"
"        // ...\n"
"    ];\n"
"}"
