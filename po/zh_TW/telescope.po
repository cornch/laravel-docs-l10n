" */\n"
"public function register(): void\n"
"{\n"
"    // ...\n"
"\n"
"    Telescope::avatar(function (string $id, string $email) {\n"
"        return '/avatars/'.User::find($id)->avatar_path;\n"
"    });\n"
"}"
msgstr ""
