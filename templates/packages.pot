# File: docs/master/packages.md, line: 270
#, no-wrap
msgid "    <x-courier-alert />\n"
msgstr ""
#
#. type: Plain text
# File: docs/8.x/packages.md, line: 272
# File: docs/master/packages.md, line: 272
#, no-wrap
msgid "    <x-courier-button />\n"
msgstr ""
