"process_name=%(program_name)s\n"
"command=php /home/forge/example.com/artisan horizon\n"
"autostart=true\n"
"autorestart=true\n"
"user=forge\n"
"redirect_stderr=true\n"
"stdout_logfile=/home/forge/example.com/horizon.log\n"
"stopwaitsecs=3600"
