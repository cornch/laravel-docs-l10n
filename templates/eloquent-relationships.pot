#. type: Plain text
# File: docs/8.x/eloquent-relationships.md, line: 825
# File: docs/master/eloquent-relationships.md, line: 825
#, no-wrap
msgid ""
"    posts\n"
"        id - integer\n"
"        title - string\n"
"        body - text\n"
msgstr ""
#
#. type: Plain text
# File: docs/8.x/eloquent-relationships.md, line: 830
# File: docs/master/eloquent-relationships.md, line: 830
#, no-wrap
msgid ""
"    videos\n"
"        id - integer\n"
"        title - string\n"
"        url - string\n"
msgstr ""
#
#. type: Plain text
# File: docs/8.x/eloquent-relationships.md, line: 836
# File: docs/master/eloquent-relationships.md, line: 836
#, no-wrap
msgid ""
"    comments\n"
"        id - integer\n"
"        body - text\n"
"        commentable_id - integer\n"
"        commentable_type - string\n"
msgstr ""
#
#. type: Plain text
# File: docs/8.x/eloquent-relationships.md, line: 838
# File: docs/master/eloquent-relationships.md, line: 838
#, no-wrap
msgid "<a name=\"one-to-many-polymorphic-model-structure\"></a>\n"
msgstr ""
#
#. type: Plain text
# File: docs/8.x/eloquent-relationships.md, line: 858
# File: docs/master/eloquent-relationships.md, line: 858
#, no-wrap
msgid ""
"    class Comment extends Model\n"
"    {\n"
"        /**\n"
"         * Get the parent commentable model (post or video).\n"
"         */\n"
"        public function commentable()\n"
"        {\n"
"            return $this->morphTo();\n"
"        }\n"
"    }\n"
msgstr ""
#
#. type: Plain text
# File: docs/8.x/eloquent-relationships.md, line: 869
# File: docs/master/eloquent-relationships.md, line: 869
#, no-wrap
msgid ""
"    class Post extends Model\n"
"    {\n"
"        /**\n"
"         * Get all of the post's comments.\n"
"         */\n"
"        public function comments()\n"
"        {\n"
"            return $this->morphMany(Comment::class, 'commentable');\n"
"        }\n"
"    }\n"
msgstr ""
#
#. type: Plain text
# File: docs/8.x/eloquent-relationships.md, line: 880
# File: docs/master/eloquent-relationships.md, line: 880
#, no-wrap
msgid ""
"    class Video extends Model\n"
