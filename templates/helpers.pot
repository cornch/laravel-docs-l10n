#
#. type: Plain text
# File: docs/8.x/helpers.md, line: 3309
# File: docs/master/helpers.md, line: 3337
#, no-wrap
msgid ""
"    return retry(5, function () {\n"
"        // ...\n"
"    }, function ($attempt) {\n"
"        return $attempt * 100;\n"
"    });\n"
msgstr ""
#
#. type: Plain text
# File: docs/8.x/helpers.md, line: 3312
# File: docs/master/helpers.md, line: 3345
