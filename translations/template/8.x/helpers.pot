#. type: Plain text
#: docs/8.x/helpers.md:2906
#, no-wrap
msgid "    $current = url()->current();\n"
msgstr ""

#. type: Plain text
#: docs/8.x/helpers.md:2908
#, no-wrap
msgid "    $full = url()->full();\n"
msgstr ""

#. type: Plain text
#: docs/8.x/helpers.md:2910
#, no-wrap
msgid "    $previous = url()->previous();\n"
msgstr ""

