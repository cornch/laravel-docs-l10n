"$collection = collect([1, 2, 3]);\n"
"\n"
"$collection->when(false, function ($collection) {\n"
"    return $collection->push(4);\n"
"}, function ($collection) {\n"
"    return $collection->push(5);\n"
"});\n"
"\n"
"$collection->all();\n"
"\n"
"// [1, 2, 3, 5]"
msgstr ""

