"            $table->string('name');\n"
"            $table->string('airline');\n"
"            $table->timestamps();\n"
"        });\n"
"    }\n\n"
"    /**\n"
"     * Reverse the migrations.\n"
"     *\n"
"     * @return void\n"
"     */\n"
