"public function test_it_checks_if_ready_four_times()\n"
"{\n"
"    Sleep::fake();\n"
"\n"
"    // ...\n"
"\n"
"    Sleep::assertSequence([\n"
"        Sleep::for(1)->second(),\n"
"        Sleep::for(2)->seconds(),\n"
"        Sleep::for(3)->seconds(),\n"
"    ]);\n"
"}"
msgstr ""

