"     * Transform the resource into an array.\n"
"     *\n"
"     * @return array<string, mixed>\n"
"     */\n"
"    public function toArray(Request $request): array\n"
"    {\n"
"        return [\n"
"            'id' => $this->id,\n"
"            'name' => $this->name,\n"
"            'email' => $this->email,\n"
"            'created_at' => $this->created_at,\n"
"            'updated_at' => $this->updated_at,\n"
