#: ./docs/11.x/helpers.md:822
#: ./docs/master/helpers.md:792
msgctxt "CODE"
msgid ""
"use Illuminate\\Support\\Arr;\n"
"\n"
"$array = ['Desk', 'Table', 'Chair'];\n"
"\n"
"$sorted = Arr::sortDesc($array);\n"
"\n"
"// ['Table', 'Desk', 'Chair']"
msgstr ""

