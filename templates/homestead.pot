msgid "Symbolic Links On Windows"
msgstr ""
#
#. type: Plain text
# File: docs/8.x/homestead.md, line: 766
# File: docs/master/homestead.md, line: 766
msgid "If symbolic links are not working properly on your Windows machine, you may need to add the following block to your `Vagrantfile`:"
msgstr ""
#
#. type: Code fence info string
# File: docs/8.x/homestead.md, line: 767
# File: docs/master/homestead.md, line: 767
#, no-wrap
msgid "ruby"
msgstr ""
#
#. type: Plain text
# File: docs/8.x/homestead.md, line: 767
# File: docs/master/homestead.md, line: 767
#, no-wrap
msgid ""
"config.vm.provider \"virtualbox\" do |v|\n"
"    v.customize [\"setextradata\", :id, \"VBoxInternal2/SharedFoldersEnableSymlinksCreate/v-root\", \"1\"]\n"
"end\n"
msgstr ""
