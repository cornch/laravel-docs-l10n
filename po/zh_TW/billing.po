"\n"
"    $request->user()->newSubscription('default', 'price_monthly')\n"
"                ->anchorBillingCycleOn($anchor->startOfDay())\n"
"                ->create($request->paymentMethodId);\n"
"\n"
"    // ...\n"
"});"
