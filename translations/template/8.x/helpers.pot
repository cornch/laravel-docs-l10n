#. type: Plain text
#: docs/8.x/helpers.md:1862
#, markdown-text, no-wrap
msgid "    $matches = Str::of('foobar')->is('foo*');\n"
msgstr ""

#. type: Plain text
#: docs/8.x/helpers.md:1866
#, markdown-text, no-wrap
msgid "    $matches = Str::of('foobar')->is('baz*');\n"
msgstr ""

