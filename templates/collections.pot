msgstr ""
#
#. type: Plain text
# File: docs/8.x/collections.md, line: 1002
# File: docs/master/collections.md, line: 1003
#, no-wrap
msgid ""
"    /*\n"
"        [\n"
"            'x10' => [\n"
"                ['account_id' => 'account-x10', 'product' => 'Chair'],\n"
"                ['account_id' => 'account-x10', 'product' => 'Bookcase'],\n"
