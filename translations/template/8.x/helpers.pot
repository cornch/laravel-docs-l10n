msgstr ""

#. type: Plain text
#: docs/8.x/helpers.md:1852
#, no-wrap
msgid "    $string = Str::of('Taylor')->append(' Otwell');\n"
msgstr ""

#. type: Plain text
#: docs/8.x/helpers.md:1854 docs/8.x/helpers.md:2649
#, no-wrap
msgid "    // 'Taylor Otwell'\n"
msgstr ""

