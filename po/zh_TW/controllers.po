"\n"
"class ProvisionServer extends Controller\n"
"{\n"
"    /**\n"
"     * Provision a new web server.\n"
"     *\n"
"     * @return \\Illuminate\\Http\\Response\n"
"     */\n"
"    public function __invoke()\n"
"    {\n"
"        // ...\n"
"    }\n"
"}"
msgstr ""

#: ./docs/10.x/controllers.md:74
#: ./docs/11.x/controllers.md:74
