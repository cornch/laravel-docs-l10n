#. type: Plain text
#: docs/8.x/helpers.md:943
#, markdown-text, no-wrap
msgid "    $path = app_path();\n"
msgstr ""

#. type: Plain text
#: docs/8.x/helpers.md:945
#, markdown-text, no-wrap
msgid "    $path = app_path('Http/Controllers/Controller.php');\n"
msgstr ""

#. type: Plain text
