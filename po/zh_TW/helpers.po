"use Illuminate\\Support\\Str;\n"
"\n"
"$string = 'Laravel 8.x';\n"
"\n"
"$replaced = Str::replace('8.x', '9.x', $string);\n"
"\n"
"// Laravel 9.x"
msgstr ""

