"        10 => ['user' => 1, 'skill' => 1, 'roles' => ['Role_1', 'Role_3']],\n"
"        20 => ['user' => 2, 'skill' => 1, 'roles' => ['Role_1', 'Role_2']],\n"
"        30 => ['user' => 3, 'skill' => 2, 'roles' => ['Role_1']],\n"
"        40 => ['user' => 4, 'skill' => 2, 'roles' => ['Role_2']],\n"
"    ]);\n"
msgstr ""
