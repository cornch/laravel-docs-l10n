"        with:\n"
"          name: screenshots\n"
"          path: tests/Browser/screenshots\n"
"      - name: Upload Console Logs\n"
"        if: failure()\n"
"        uses: actions/upload-artifact@v2\n"
"        with:\n"
"          name: console\n"
"          path: tests/Browser/console\n"
