#. type: Title ####
#: docs/8.x/helpers.md:2354
#, no-wrap
msgid "`ucfirst` {#collection-method}"
msgstr ""

#. type: Plain text
#: docs/8.x/helpers.md:2357
msgid ""
"The `ucfirst` method returns the given string with the first character "
"capitalized:"
msgstr ""

#. type: Plain text
#: docs/8.x/helpers.md:2361
#, no-wrap
msgid "    $string = Str::of('foo bar')->ucfirst();\n"
msgstr ""

#. type: Plain text
#: docs/8.x/helpers.md:2365
#, no-wrap
msgid "<a name=\"method-fluent-str-upper\"></a>\n"
msgstr ""

#. type: Title ####
#: docs/8.x/helpers.md:2365
#, no-wrap
msgid "`upper` {#collection-method}"
msgstr ""

#. type: Plain text
#: docs/8.x/helpers.md:2368
