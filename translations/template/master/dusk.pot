#. type: Plain text
#: docs/master/dusk.md:925
#, no-wrap
msgid "<a name=\"assert-title\"></a>\n"
msgstr ""

#. type: Title ####
#: docs/master/dusk.md:925
#, no-wrap
msgid "assertTitle"
msgstr ""

#. type: Plain text
#: docs/master/dusk.md:928
msgid "Assert that the page title matches the given text:"
msgstr ""

#. type: Plain text
#: docs/master/dusk.md:930
#, no-wrap
msgid "    $browser->assertTitle($title);\n"
msgstr ""

#. type: Plain text
#: docs/master/dusk.md:932
#, no-wrap
msgid "<a name=\"assert-title-contains\"></a>\n"
msgstr ""

#. type: Title ####
#: docs/master/dusk.md:932
#, no-wrap
msgid "assertTitleContains"
msgstr ""

#. type: Plain text
#: docs/master/dusk.md:935
msgid "Assert that the page title contains the given text:"
msgstr ""

#. type: Plain text
#: docs/master/dusk.md:937
#, no-wrap
msgid "    $browser->assertTitleContains($title);\n"
msgstr ""

#. type: Plain text
#: docs/master/dusk.md:939
#, no-wrap
msgid "<a name=\"assert-url-is\"></a>\n"
msgstr ""

#. type: Title ####
#: docs/master/dusk.md:939
#, no-wrap
msgid "assertUrlIs"
msgstr ""

#. type: Plain text
#: docs/master/dusk.md:942
msgid ""
"Assert that the current URL (without the query string) matches the given "
"string:"
msgstr ""

#. type: Plain text
#: docs/master/dusk.md:944
#, no-wrap
msgid "    $browser->assertUrlIs($url);\n"
msgstr ""

#. type: Plain text
#: docs/master/dusk.md:946
#, no-wrap
msgid "<a name=\"assert-scheme-is\"></a>\n"
msgstr ""

#. type: Title ####
#: docs/master/dusk.md:946
#, no-wrap
msgid "assertSchemeIs"
