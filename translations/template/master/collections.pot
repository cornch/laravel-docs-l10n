#. type: Plain text
#: docs/master/collections.md:255
#, markdown-text
msgid ""
"The `avg` method returns the [average "
"value](https://en.wikipedia.org/wiki/Average) of a given key:"
msgstr ""

#. type: Plain text
#: docs/master/collections.md:262
#, markdown-text, no-wrap
msgid ""
"    $average = collect([\n"
"        ['foo' => 10],\n"
"        ['foo' => 10],\n"
"        ['foo' => 20],\n"
"        ['foo' => 40]\n"
"    ])->avg('foo');\n"
msgstr ""

#. type: Plain text
#: docs/master/collections.md:264 docs/master/collections.md:1313
#, markdown-text, no-wrap
msgid "    // 20\n"
msgstr ""

#. type: Plain text
#: docs/master/collections.md:266
#, markdown-text, no-wrap
msgid "    $average = collect([1, 1, 2, 4])->avg();\n"
msgstr ""

#. type: Plain text
#: docs/master/collections.md:268 docs/master/collections.md:1156
#: docs/master/collections.md:1827
#, markdown-text, no-wrap
msgid "    // 2\n"
