#: docs/8.x/helpers.md:2416
#, no-wrap
msgid ""
"    $string = Str::of('Laravel')\n"
"        ->append(' Framework')\n"
"        ->tap(function ($string) {\n"
"            dump('String after append: ' . $string);\n"
"        })\n"
"        ->upper();\n"
msgstr ""

#. type: Plain text
#: docs/8.x/helpers.md:2418
#, no-wrap
msgid "    // LARAVEL FRAMEWORK\n"
msgstr ""

