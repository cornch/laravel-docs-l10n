"\n"
"$validator = Validator::make($data, [\n"
"    'has_appointment' => 'required|boolean',\n"
"    'appointment_date' => 'exclude_if:has_appointment,false|required|date',\n"
"    'doctor_name' => 'exclude_if:has_appointment,false|required|string',\n"
"]);"
