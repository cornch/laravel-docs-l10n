msgid ""
"use Illuminate\\Support\\Facades\\Validator;\n"
"use Closure;\n"
"\n"
"$validator = Validator::make($request->all(), [\n"
"    'title' => [\n"
"        'required',\n"
"        'max:255',\n"
"        function (string $attribute, mixed $value, Closure $fail) {\n"
"            if ($value === 'foo') {\n"
"                $fail(\"The {$attribute} is invalid.\");\n"
"            }\n"
"        },\n"
"    ],\n"
"]);"
msgstr ""

