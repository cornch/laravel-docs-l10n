#. type: Plain text
#: docs/8.x/helpers.md:2753
#, markdown-text, no-wrap
msgid ""
"    filled('');\n"
"    filled('   ');\n"
"    filled(null);\n"
"    filled(collect());\n"
msgstr ""

#. type: Plain text
#: docs/8.x/helpers.md:2757
#, markdown-text
