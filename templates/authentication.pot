msgstr ""

#: ./docs/9.x/authentication.md:294
msgid ""
"if (Auth::attemptWhen([\n"
"    'email' => $email,\n"
"    'password' => $password,\n"
"], function ($user) {\n"
"    return $user->isNotBanned();\n"
"})) {\n"
"    // Authentication was successful...\n"
"}"
msgstr ""

