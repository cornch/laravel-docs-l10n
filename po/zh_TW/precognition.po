"    />\n"
"    <template x-if=\"form.invalid('email')\">\n"
"        <div x-text=\"form.errors.email\"></div>\n"
"    </template>\n"
"\n"
"    <button :disabled=\"form.processing\">\n"
"        Create User\n"
"    </button>\n"
