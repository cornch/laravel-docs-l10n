msgstr ""

#. type: Plain text
#: docs/master/helpers.md:2551
#, markdown-text, no-wrap
msgid "<a name=\"method-blank\"></a>\n"
msgstr ""

#. type: Title ####
#: docs/master/helpers.md:2551
#, markdown-text, no-wrap
msgid "`blank()` {#collection-method}"
msgstr ""

#. type: Plain text
#: docs/master/helpers.md:2554
#, markdown-text
msgid "The `blank` function determines whether the given value is \"blank\":"
msgstr ""

#. type: Plain text
#: docs/master/helpers.md:2559
#, markdown-text, no-wrap
msgid ""
"    blank('');\n"
"    blank('   ');\n"
"    blank(null);\n"
"    blank(collect());\n"
