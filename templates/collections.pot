#. type: Plain text
# File: docs/8.x/collections.md, line: 1716
# File: docs/master/collections.md, line: 1717
#, no-wrap
msgid "    // 'Desk'\n"
msgstr ""
#
#. type: Plain text
# File: docs/8.x/collections.md, line: 1720
# File: docs/master/collections.md, line: 1721
#, no-wrap
msgid "    // ['product_id' => 'prod-100']\n"
