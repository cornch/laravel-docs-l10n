#, no-wrap
msgid "    // VerifiedHumans\n"
msgstr ""

#. type: Plain text
#: docs/8.x/helpers.md:1508
#, no-wrap
msgid "    $plural = Str::pluralStudly('UserFeedback');\n"
msgstr ""

#. type: Plain text
#: docs/8.x/helpers.md:1510
#, no-wrap
msgid "    // UserFeedback\n"
msgstr ""

#. type: Plain text
#: docs/8.x/helpers.md:1516
#, no-wrap
msgid "    $plural = Str::pluralStudly('VerifiedHuman', 2);\n"
msgstr ""

#. type: Plain text
#: docs/8.x/helpers.md:1520
#, no-wrap
msgid "    $singular = Str::pluralStudly('VerifiedHuman', 1);\n"
msgstr ""

#. type: Plain text
#: docs/8.x/helpers.md:1522
#, no-wrap
msgid "    // VerifiedHuman\n"
msgstr ""

