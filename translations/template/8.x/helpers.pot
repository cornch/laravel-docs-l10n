msgstr ""

#. type: Plain text
#: docs/8.x/helpers.md:2685
#, markdown-text, no-wrap
msgid "<a name=\"method-dd\"></a>\n"
msgstr ""

#. type: Title ####
#: docs/8.x/helpers.md:2685
#, markdown-text, no-wrap
msgid "`dd()` {#collection-method}"
msgstr ""

#. type: Plain text
#: docs/8.x/helpers.md:2688
#, markdown-text
msgid ""
"The `dd` function dumps the given variables and ends execution of the "
"script:"
msgstr ""

#. type: Plain text
#: docs/8.x/helpers.md:2690
#, markdown-text, no-wrap
msgid "    dd($value);\n"
msgstr ""

#. type: Plain text
#: docs/8.x/helpers.md:2692
#, markdown-text, no-wrap
msgid "    dd($value1, $value2, $value3, ...);\n"
msgstr ""

#. type: Plain text
#: docs/8.x/helpers.md:2694
#, markdown-text
