msgstr ""

#: ./docs/8.x/telescope.md:112
#: ./docs/9.x/telescope.md:120
#: ./docs/10.x/telescope.md:120
#: ./docs/11.x/telescope.md:117
#: ./docs/master/telescope.md:117
msgctxt "P"
msgid "By default, all entries older than 24 hours will be pruned. You may use the `hours` option when calling the command to determine how long to retain Telescope data. For example, the following command will delete all records created over 48 hours ago:"
msgstr ""

#: ./docs/11.x/telescope.md:113
#: ./docs/master/telescope.md:113
msgctxt "CODE"
msgid "use Illuminate\\Support\\Facades\\Schedule;\n\n"
"Schedule::command('telescope:prune')->daily();"
msgstr ""

#: ./docs/8.x/telescope.md:114
#: ./docs/9.x/telescope.md:122
#: ./docs/10.x/telescope.md:122
msgctxt "CODE"
msgid "$schedule->command('telescope:prune --hours=48')->daily();"
msgstr ""

