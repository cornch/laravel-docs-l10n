#. type: Plain text
#: docs/8.x/helpers.md:2374
#, no-wrap
msgid "<a name=\"method-fluent-str-trim\"></a>\n"
msgstr ""

#. type: Title ####
#: docs/8.x/helpers.md:2374
#, no-wrap
msgid "`trim` {#collection-method}"
msgstr ""

#. type: Plain text
#: docs/8.x/helpers.md:2377
