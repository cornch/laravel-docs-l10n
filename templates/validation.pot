# File: docs/master/validation.md, line: 1371
# File: docs/master/validation.md, line: 1478
#, no-wrap
msgid ""
"    use Illuminate\\Support\\Facades\\Validator;\n"
"    use Illuminate\\Validation\\Rule;\n"
