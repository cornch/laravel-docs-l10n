#. type: Plain text
#: docs/master/collections.md:338
#, no-wrap
msgid "    $collectionA = collect([1, 2, 3]);\n"
msgstr ""

#. type: Plain text
#: docs/master/collections.md:340
#, no-wrap
msgid "    $collectionB = $collectionA->collect();\n"
msgstr ""

#. type: Plain text
#: docs/master/collections.md:342
#, no-wrap
msgid "    $collectionB->all();\n"
msgstr ""

#. type: Plain text
#: docs/master/collections.md:346
msgid ""
"The `collect` method is primarily useful for converting [lazy "
"collections](#lazy-collections) into standard `Collection` instances:"
msgstr ""

#. type: Plain text
#: docs/master/collections.md:352
#, no-wrap
msgid ""
"    $lazyCollection = LazyCollection::make(function () {\n"
"        yield 1;\n"
"        yield 2;\n"
"        yield 3;\n"
"    });\n"
msgstr ""

#. type: Plain text
#: docs/master/collections.md:354
#, no-wrap
msgid "    $collection = $lazyCollection->collect();\n"
msgstr ""

#. type: Plain text
#: docs/master/collections.md:356
#, no-wrap
msgid "    get_class($collection);\n"
msgstr ""

#. type: Plain text
#: docs/master/collections.md:358
#, no-wrap
msgid "    // 'Illuminate\\Support\\Collection'\n"
